{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "lib": ["esnext", "dom"]
  },
  "include": [
    ".vitepress/**/*.ts",
    ".vitepress/**/*.mts",
    ".vitepress/**/*.data.ts",
    ".vitepress/**/*.vue",
    "eslint.config.mjs"
  ],
  "exclude": ["dist", "node_modules"]
}
